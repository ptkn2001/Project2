<div class="login">
    <div class="login-header">
        <h2>User Login</h2>
    </div>
    <div class="login-body">
        <form class="form login-form" >
            <div class="form-group">
                <label for="email-login">email:</label>
                <input class="form-input" type="text" id="email-login" />
            </div>
            <div class="form-group">
                <label for="password-login">password:</label>
                <input class="form-input" type="password" id="password-login" />
            </div>
            <div class="form-group">
                <button class="btn btn-primary" type="submit">login</button>
            </div>
        </form>
    </div>
 <hr>
 <button id="sign-in-or-out-button"
        style="margin-left: 25px">Google Signin</button>
         
 <hr>
    <div class="login-signup">
        <a href="/signup">Create a new user account</a>
    </div>
</div>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script src="./js/gscript.js"></script>
<script async defer src="https://apis.google.com/js/api.js"
        onload="this.onload=function(){};handleClientLoad()"
        onreadystatechange="if (this.readyState === 'complete') this.onload()">
</script>




<script src="./js/login.js"></script>